<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <path>Gauges.ac</path>
  
  <animation>
    <object-name>fond</object-name>
    <object-name>AmpsPtr</object-name>
    <object-name>CylTempPtr</object-name>
    <object-name>FuelPtr</object-name>
    <object-name>OilPressPtr</object-name>
    <object-name>OilTempPtr</object-name>
    <object-name>VoltsPtr</object-name>
    
    <object-name>vitre</object-name>
  </animation>
  
  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Aircraft/Grob-G109/Models/Effects/Glass/glass</inherits-from>
    <object-name>vitre</object-name>
  </effect>
  
  <animation>
    <type>material</type>
    <object-name>fond</object-name>
    <object-name>AmpsPtr</object-name>
    <object-name>CylTempPtr</object-name>
    <object-name>FuelPtr</object-name>
    <object-name>OilPressPtr</object-name>
    <object-name>OilTempPtr</object-name>
    <object-name>VoltsPtr</object-name>
    <emission>
      <factor-prop>/controls/lighting/instruments-norm</factor-prop>
      <red>   0.9 </red>
      <green> 0.5 </green>
      <blue>  0.2 </blue>
    </emission>
  </animation>
  
  <animation> 
    <type>rotate</type>
    <object-name>FuelPtr</object-name>
    <property>consumables/fuel/tank[0]/level-lbs</property>
    <interpolation>
      <entry><ind>   0 </ind><dep>  0 </dep></entry>
      <entry><ind> 158 </ind><dep> 90 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.000 </x-m>
      <y-m> -0.100 </y-m>
      <z-m> -0.005 </z-m>  
    </center>
    <axis> 
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>         
  
  <animation> 
    <type>rotate</type>
    <object-name>CylTempPtr</object-name>
    <property>consumables/fuel/tank[0]/level-lbs</property>
    <interpolation>
      <entry><ind>   0 </ind><dep>  0 </dep></entry>
      <entry><ind> 158 </ind><dep> 90 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.000 </x-m>
      <y-m> -0.060 </y-m>
      <z-m> -0.005 </z-m>  
    </center>
    <axis> 
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>         
  
  <animation> 
    <type>rotate</type>
    <object-name>OilPressPtr</object-name>
    <property>consumables/fuel/tank[0]/level-lbs</property>
    <interpolation>
      <entry><ind>   0 </ind><dep>  0 </dep></entry>
      <entry><ind> 158 </ind><dep> 90 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.000 </x-m>
      <y-m> -0.020 </y-m>
      <z-m> -0.005 </z-m>  
    </center>
    <axis> 
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>         
  
  <animation> 
    <type>rotate</type>
    <object-name>OilTempPtr</object-name>
    <property>consumables/fuel/tank[0]/level-lbs</property>
    <interpolation>
      <entry><ind>   0 </ind><dep>  0 </dep></entry>
      <entry><ind> 158 </ind><dep> 90 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.000 </x-m>
      <y-m>  0.020 </y-m>
      <z-m> -0.005 </z-m>  
    </center>
    <axis> 
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>         
  
  <animation> 
    <type>rotate</type>
    <object-name>VoltsPtr</object-name>
    <property>systems/electrical/volts</property>
    <interpolation>
      <entry><ind>  0 </ind><dep>  0 </dep></entry>
      <entry><ind> 40 </ind><dep> 90 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.000 </x-m>
      <y-m>  0.060 </y-m>
      <z-m> -0.005 </z-m>  
    </center>
    <axis> 
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>         
  
  <animation>
    <type>rotate</type>
    <object-name>AmpsPtr</object-name>
    <property>systems/electrical/amps</property>
    <interpolation>  
      <entry><ind> -40 </ind><dep> -45 </dep></entry>
      <entry><ind>   0 </ind><dep>   0 </dep></entry>
      <entry><ind>  40 </ind><dep>  45 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.000 </x-m>
      <y-m>  0.100 </y-m>
      <z-m> -0.005 </z-m>  
    </center>
    <axis> 
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>     
  
</PropertyList>
