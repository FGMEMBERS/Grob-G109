<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <path>ASI.ac</path>
  
  <animation>
    <object-name>Bezel</object-name>
    <object-name>Face</object-name>
    <object-name>PointerTip</object-name>
    
    <object-name>vitre</object-name>
  </animation>
  
  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Aircraft/Grob-G109/Models/Effects/Glass/glass</inherits-from>
    <object-name>vitre</object-name>
  </effect>
  
  <animation>
    <type>material</type>
    <object-name>Face</object-name>
    <object-name>PointerTip</object-name>
    <emission>
      <factor-prop>/controls/lighting/instruments-norm</factor-prop>
      <red>   0.9 </red>
      <green> 0.5 </green>
      <blue>  0.2 </blue>
    </emission>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>PointerTip</object-name>
    <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
    <interpolation>
      <entry><ind>   0 </ind><dep>   0 </dep></entry>
      <entry><ind>  30 </ind><dep>   0 </dep></entry>
      <entry><ind>  40 </ind><dep>  30 </dep></entry>
      <entry><ind> 140 </ind><dep> 330 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
</PropertyList>
