 <animation>
  <type>pick</type>
  <visible>false</visible>
  <object-name>IgnitionUp</object-name>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>nasal</command>
    <script>controls.stepMagnetos(1)</script>
   </binding>
   <mod-up>
    <binding>
     <command>nasal</command>
     <script>controls.stepMagnetos(0)</script>
    </binding>
   </mod-up>                         
  </action>
 </animation>

 <animation>
  <type>pick</type>
  <visible>false</visible>
  <object-name>IgnitionDn</object-name>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>nasal</command>
    <script>controls.stepMagnetos(-1)</script>
   </binding>
   <mod-up>
    <binding>
     <command>nasal</command>
     <script>controls.stepMagnetos(0)</script>
    </binding>
   </mod-up>                         
  </action>
 </animation>

 <animation>
  <type>select</type>
  <object-name>Key</object-name>
  <condition>
  <greater-than>
   <property>/controls/switches/ignition</property>
   <value>0</value>
  </greater-than>
 </condition>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>IgnitionSwitch</object-name>
  <object-name>Key</object-name>
  <property>/controls/switches/ignition</property>
  <interpolation>
   <entry><ind> 0</ind><dep> 0</dep></entry>
   <entry><ind> 1</ind><dep> 0</dep></entry>
   <entry><ind> 2</ind><dep> 50</dep></entry>
   <entry><ind> 3</ind><dep> 100</dep></entry>
   <entry><ind> 4</ind><dep> 150</dep></entry>
  </interpolation>
  <center>
   <x-m>0.009</x-m>
   <y-m>0.042472</y-m>
   <z-m>-0.027703</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>
