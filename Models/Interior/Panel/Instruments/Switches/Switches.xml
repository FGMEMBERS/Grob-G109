<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <path>Switches.ac</path>
  
  <animation>
    <object-name>fond</object-name>
    <object-name>IgnitionSwitch</object-name>
    <object-name>Key</object-name>
    <object-name>UH</object-name>
    <object-name>MainSwitch</object-name>
    <object-name>FuelSwitch</object-name>
    <object-name>NavSwitch</object-name>
    <object-name>StrobeSwitch</object-name>
    <object-name>LandingSwitch</object-name>
    <object-name>InstrSwitch</object-name>
    
    <object-name>MainOn</object-name>
    <object-name>FuelOn</object-name>
    <object-name>NavOn</object-name>
    <object-name>MainOff</object-name>
    <object-name>FuelOff</object-name>
    <object-name>NavOff</object-name>
    <object-name>StrobeOn</object-name>
    <object-name>LandingOn</object-name>
    <object-name>InstrOn</object-name>
    <object-name>StrobeOff</object-name>
    <object-name>LandingOff</object-name>
    <object-name>InstrOFf</object-name>
    <object-name>IgnitionDn</object-name>
    <object-name>IgnitionUp</object-name>
  </animation>
  
  <animation>
    <type>material</type>
    <object-name>MainSwitch</object-name>
    <object-name>FuelSwitch</object-name>
    <object-name>NavSwitch</object-name>
    <object-name>StrobeSwitch</object-name>
    <object-name>LandingSwitch</object-name>
    <object-name>InstrSwitch</object-name>
    <emission>
      <factor-prop>/controls/lighting/instruments-norm</factor-prop>
      <red>   0.9 </red>
      <green> 0.5 </green>
      <blue>  0.2 </blue>
    </emission>
  </animation>
  
  <animation>
    <type>pick</type>
    <visible>false</visible>
    <object-name>MainOn</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-assign</command>
        <property>controls/switches/main</property>
        <value type="bool">true</value>
      </binding>          
    </action>
  </animation>
  
  <animation>
    <type>pick</type>
    <visible>false</visible>
    <object-name>MainOff</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-assign</command>
        <property>controls/switches/main</property>
        <value type="bool">false</value>
      </binding>          
    </action>
  </animation>
  
  <animation> 
    <type>rotate</type>
    <object-name>MainSwitch</object-name>
    <property>controls/switches/main</property>
    <factor> 30 </factor>
    <center>
      <x-m> 0.003 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.020 </z-m>  
    </center>
    <axis> 
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>pick</type>
    <visible>false</visible>
    <object-name>FuelOn</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-assign</command>
        <property>controls/switches/fuel</property>
        <value type="bool">true</value>
      </binding>          
    </action>
  </animation>
  
  <animation>
    <type>pick</type>
    <visible>false</visible>
    <object-name>FuelOff</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-assign</command>
        <property>controls/switches/fuel</property>
        <value type="bool">false</value>
      </binding>          
    </action>
  </animation>
  
  <animation> 
    <type>rotate</type>
    <object-name>FuelSwitch</object-name>
    <property>controls/switches/fuel</property>
    <factor> 30 </factor>
    <center>
      <x-m> 0.003 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.020 </z-m>  
    </center>
    <axis> 
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>pick</type>
    <visible>false</visible>
    <object-name>NavOn</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-assign</command>
        <property>controls/switches/navlight</property>
        <value type="bool">true</value>
      </binding>          
    </action>
  </animation>
  
  <animation>
    <type>pick</type>
    <visible>false</visible>
    <object-name>NavOff</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-assign</command>
        <property>controls/switches/navlight</property>
        <value type="bool">false</value>
      </binding>          
    </action>
  </animation>
  
  <animation> 
    <type>rotate</type>
    <object-name>NavSwitch</object-name>
    <property>controls/switches/navlight</property>
    <factor> 30 </factor>
    <center>
      <x-m> 0.003 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.020 </z-m>  
    </center>
    <axis> 
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  
  <animation>
    <type>pick</type>
    <visible>false</visible>
    <object-name>StrobeOn</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-assign</command>
        <property>controls/switches/strobelight</property>
        <value type="bool">true</value>
      </binding>          
    </action>
  </animation>
  
  <animation>
    <type>pick</type>
    <visible>false</visible>
    <object-name>StrobeOff</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-assign</command>
        <property>controls/switches/strobelight</property>
        <value type="bool">false</value>
      </binding>          
    </action>
  </animation>
  
  <animation> 
    <type>rotate</type>
    <object-name>StrobeSwitch</object-name>
    <property>controls/switches/strobelight</property>
    <factor> 30 </factor>
    <center>
      <x-m>  0.003 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.020 </z-m>  
    </center>
    <axis> 
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>pick</type>
    <visible>false</visible>
    <object-name>LandingOn</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-assign</command>
        <property>controls/switches/landinglight</property>
        <value type="bool">true</value>
      </binding>          
    </action>
  </animation>
  
  <animation>
    <type>pick</type>
    <visible>false</visible>
    <object-name>LandingOff</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-assign</command>
        <property>controls/switches/landinglight</property>
        <value type="bool">false</value>
      </binding>          
    </action>
  </animation>
  
  <animation> 
    <type>rotate</type>
    <object-name>LandingSwitch</object-name>
    <property>controls/switches/landinglight</property>
    <factor> 30 </factor>
    <center>
      <x-m>  0.003 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.020 </z-m>  
    </center>
    <axis> 
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>pick</type>
    <visible>false</visible>
    <object-name>InstrOn</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-assign</command>
        <property>controls/switches/instrumentlight</property>
        <value type="bool">true</value>
      </binding>          
    </action>
  </animation>
  
  <animation>
    <type>pick</type>
    <visible>false</visible>
    <object-name>InstrOff</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-assign</command>
        <property>controls/switches/instrumentlight</property>
        <value type="bool">false</value>
      </binding>          
    </action>
  </animation>
  
  <animation> 
    <type>rotate</type>
    <object-name>InstrSwitch</object-name>
    <property>controls/switches/instrumentlight</property>
    <factor> 30 </factor>
    <center>
      <x-m>  0.003 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.020 </z-m>  
    </center>
    <axis> 
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>pick</type>
    <visible>false</visible>
    <object-name>IgnitionUp</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>controls.stepMagnetos(1)</script>
      </binding>
      <mod-up>
        <binding>
          <command>nasal</command>
          <script>controls.stepMagnetos(0)</script>
        </binding>
      </mod-up>                         
    </action>
  </animation>
  
  <animation>
    <type>pick</type>
    <visible>false</visible>
    <object-name>IgnitionDn</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>controls.stepMagnetos(-1)</script>
      </binding>
      <mod-up>
        <binding>
          <command>nasal</command>
          <script>controls.stepMagnetos(0)</script>
        </binding>
      </mod-up>                         
    </action>
  </animation>
  
  <animation>
    <type>select</type>
    <object-name>Key</object-name>
    <condition>
      <greater-than>
        <property>/controls/switches/ignition</property>
        <value>0</value>
      </greater-than>
    </condition>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>IgnitionSwitch</object-name>
    <object-name>Key</object-name>
    <property>/controls/switches/ignition</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>   0 </dep></entry>
      <entry><ind> 1 </ind><dep>   0 </dep></entry>
      <entry><ind> 2 </ind><dep>  50 </dep></entry>
      <entry><ind> 3 </ind><dep> 100 </dep></entry>
      <entry><ind> 4 </ind><dep> 150 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.009000 </x-m>
      <y-m>  0.042472 </y-m>
      <z-m> -0.027703 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
</PropertyList>
