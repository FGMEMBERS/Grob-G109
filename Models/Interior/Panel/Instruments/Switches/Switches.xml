<?xml version="1.0"?>

<PropertyList>

 <path>Switches.ac</path>

<animation>
  <type>pick</type>
  <visible>false</visible>
  <object-name>MainOn</object-name>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>controls/switches/main</property>
    <value type="bool">true</value>
   </binding>          
  </action>
 </animation>

<animation>
  <type>pick</type>
  <visible>false</visible>
  <object-name>MainOff</object-name>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>controls/switches/main</property>
    <value type="bool">false</value>
   </binding>          
  </action>
 </animation>

 <animation> 
  <type>rotate</type>
  <object-name>MainSwitch</object-name>
  <property>controls/switches/main</property>
  <factor>30</factor>
  <axis> 
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0.003</x-m>
   <y-m>0</y-m>
   <z-m>0.020</z-m>  
  </center>
 </animation>

