    <axis>
      <x>1.0</x>
      <y>0.0</y>
      <z>0.0</z>
    </axis>
  </animation>

        <animation>
                <type>rotate</type>
                <object-name>TrimHandle</object-name>
                <object-name>TrimLever</object-name>
                <property>controls/flight/elevator-trim</property>
                <factor>25</factor>
                <center>
                        <x-m>0.44</x-m>
                        <y-m>-0.015</y-m>
                        <z-m>-0.267194</z-m>
                </center>
                <axis>
                        <x>0</x>
                        <y>-0.2</y>
                        <z>0</z>
                </axis>
        </animation>

        <animation>
                <type>rotate</type>
                <object-name>ThrottleHandle</object-name>
                <object-name>ThrottleLever</object-name>
                <property>controls/engines/engine[0]/throttle</property>
                <factor>80</factor>
                <center>
                        <x-m>0.44</x-m>
                        <y-m>-0.015</y-m>
                        <z-m>-0.267194</z-m>
                </center>
                <axis>
