<?xml version="1.0"?>

<PropertyList>

 <path>EnginePanel.ac</path>

 <animation>
   <type>pick</type>
   <visible>false</visible>
   <object-name>ParkingBrakeOff</object-name>
   <action>
    <button>0</button>
    <repeatable>true</repeatable>
    <binding>
      <command>property-adjust</command>
      <property>controls/gear/brake-parking</property>
      <step type="double">-0.05</step>
    </binding>          
   </action>
  </animation>

 <animation>
   <type>pick</type>
   <visible>false</visible>
   <object-name>ParkingBrakeOn</object-name>
   <action>
    <button>0</button>
    <repeatable>true</repeatable>
    <binding>
      <command>property-adjust</command>
      <property>controls/gear/brake-parking</property>
      <step type="double">0.05</step>
    </binding>          
   </action>
  </animation>

  <animation>
   <type>translate</type>
   <object-name>ParkingBrake</object-name>
   <object-name>ParkingBrakeShaft</object-name>
   <property>controls/gear/brake-parking</property>
   <factor>0.04</factor>
   <axis>
    <x>1.0</x>
    <y>0.0</y>
    <z>0.0</z>
   </axis>
  </animation>

  <animation>
   <type>pick</type>
   <visible>true</visible>
   <object-name>CarbHeat</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-toggle</command>
      <property>controls/anti-ice/carb-heat</property>
    </binding>          
   </action>
  </animation>

  <animation>
   <type>translate</type>
   <object-name>CarbHeat</object-name>
   <object-name>CarbHeatShaft</object-name>
   <property>controls/anti-ice/carb-heat</property>
   <factor>0.04</factor>
   <axis>
    <x>1.0</x>
    <y>0.0</y>
    <z>0.0</z>
   </axis>
  </animation>

  <animation>
   <type>pick</type>
   <visible>true</visible>
   <object-name>CabinHeat</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-toggle</command>
      <property>controls/heater</property>
    </binding>          
   </action>
  </animation>

  <animation>
   <type>translate</type>
   <object-name>CabinHeat</object-name>
   <object-name>CabinHeatShaft</object-name>
   <property>controls/heater</property>
   <factor>0.04</factor>
   <axis>
    <x>1.0</x>
    <y>0.0</y>
    <z>0.0</z>
   </axis>
  </animation>

  <animation>
   <type>pick</type>
   <visible>true</visible>
   <object-name>Choke</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-toggle</command>
      <property>controls/engines/engine[0]/choke</property>
    </binding>          
   </action>
  </animation>

  <animation>
   <type>translate</type>
   <object-name>Choke</object-name>
   <object-name>ChokeShaft</object-name>
   <property>controls/engines/engine[0]/choke</property>
   <factor>0.04</factor>
   <axis>
    <x>1.0</x>
    <y>0.0</y>
    <z>0.0</z>
   </axis>
  </animation>

</PropertyList>
